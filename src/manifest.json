{
   "name": "Scoutbook Buddy - SBFA Remix",
   "short_name": "SB Buddy",
   "version": "0.1.0",
   "version_name": "0.44.0.7-REPACKAGE",
   "manifest_version": 2,
   "author": "Gary Feutz - Repackaged by Adam A. Koch (aakoch)",
   "description": "Repackaging of Feature Assistant for Scoutbook: Extends Scoutbook functionality with calendar, quick entry, and summer camp import functions",
   "icons": {
    "16": "icons/icon_16.png",
    "24": "icons/icon_24.png",
    "32": "icons/icon_32.png",
    "36": "icons/icon_36.png",
    "48": "icons/icon_48.png",
    "64": "icons/icon_64.png"
  },
   "default_locale": "en",
   "background": {
      "scripts": [
         "sbbackground.js",
         "tabIndicator.js"
     ]
   },
   "permissions": [
     "tabs",
     "storage",
     "http://*.scoutbook.com/*",
     "https://*.scoutbook.com/*"
   ],
   "options_page": "options.html",
   "content_scripts": [
     {
       "matches": [
         "http://*.scoutbook.com/*",
         "https://*.scoutbook.com/*"
       ],
       "js": [
         "contentscript.js"
       ],
       "run_at": "document_idle",
       "all_frames": false
     },
     {
        "js": [
           "sbcontent.js"
        ],
        "matches": [
           "https://*.scoutbook.com/*"
        ],
        "run_at": "document_start"
     }
   ],
   "browser_action": {
     "default_icon": {
       "16": "icons/icon_16.png",
       "24": "icons/icon_24.png",
       "32": "icons/icon_32.png",
       "36": "icons/icon_36.png",
       "48": "icons/icon_48.png",
       "64": "icons/icon_64.png"
     },
     "default_title": "Scoutbook Buddy",
     "default_popup": "popup.html"
   },
   "web_accessible_resources": [
     "scripts/inject.js",
     "scripts/preview.js",
      "sbutils.js",
      "copyevent.js",
      "common.js",
      "registrationimport.js",
      "eventimport.js",
      "paybalancereport.js",
      "evmsg.js",
      "healthqe.js",
      "oaqe.js",
      "blackpugimport.js",
      "csvprocs.js",
      "addinvitees.js",
      "localsession.js",
      "autologs.js",
      "recur.js",
      "mbc.js",
      "swimqe.js",
      "mbadd.js",
      "payqe.js",
      "rsvpreport.js",
      "oareport.js",
      "connmgr.js",
      "help.js",
      "grades.js",
      "positionpermission.js",
      "trainingQE.js",
      "youthLeaderQE.js",
      "messageSubSelect.js",
      "mbcPruneConn.js",
      "customMailList.js",
      "birthday.js",
      "rosterPrintSort.js",
      "parentImport.js",
      "adultYPTreport.js",
      "mbcSearchConnect.js",
      "mblist.js",
      "emailedit48.png",
      "emaillook48.png",
      "editbc48.png",
      "undo.js",
      "councilUser.js",
      "eventexport.js",
      "bindings.js"
   ],
   "externally_connectable": {
     "matches": ["*://*.scoutbook.com/*"]
   }
}